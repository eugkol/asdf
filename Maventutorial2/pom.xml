	</properties>




	<developers>
		<developer>
			<id>balaji</id>
			<name>Balaji Varanasi</name>
			<email>balaji@inflinx.com</email>
			<properties>
				<active>true</active>
			</properties>
		</developer>
		<developer>
			<id>sudha</id>
			<name>Sudha Belida</name>
			<email>sudha@inflinx.com</email>
